{
    "dependencies": {
        "NetTelegramBotApi": "4.1.0",
        "Microsoft.EntityFrameworkCore": "1.1.2",
        "Microsoft.EntityFrameworkCore.Sqlite": "1.1.2",
        "Microsoft.AspNetCore.Routing": "1.1.2",
        "Microsoft.AspNetCore.Mvc": "1.1.2",
        "Microsoft.AspNetCore.Server.Kestrel": "1.1.2",
        "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.2",
        "Microsoft.Extensions.Configuration.FileExtensions": "1.1.2",
        "Microsoft.Extensions.Configuration.Json": "1.1.2",
        "Microsoft.Extensions.Logging": "1.1.2",
        "Microsoft.Extensions.Logging.Abstractions": "1.1.2",
        "Microsoft.Extensions.Logging.Console": "1.1.2",
        "Microsoft.Extensions.Logging.Debug": "1.1.2",
        "Microsoft.Extensions.Options.ConfigurationExtensions": "1.1.2",
        "Microsoft.NETCore.App": {
            "version": "1.0.1",
            "type": "platform"
        },
        "NGitLab": "2.0.0-beta-0065",
        "NLog.Web.AspNetCore": "4.4.1",
        "NLog.Targets.ElasticSearch": "4.0.0-beta21",
        "NLog.Extensions.Logging": "1.0.0-rtm-beta5"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "imports": [
                "dotnet5.6",
                "portable-net45+win8"
            ]
        }
    },

    "buildOptions": {
        "emitEntryPoint": true,
        "preserveCompilationContext": true,
        "copyToOutput": { "includeFiles": [ "nlog.config" ] }
    },

    "runtimeOptions": {
        "configProperties": {
            "System.GC.Server": true
        }
    },

    "publishOptions": {
        "include": [
            "wwwroot",
            "**/*.cshtml",
            "appsettings.json",
            "nlog.config",
            "web.config"
        ]
    }
}
